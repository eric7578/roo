{
  "manifest_version": 2,
  "name": "roo",
  "description": "github viewer",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "management"
  ],
  "background": {
    "scripts": [
      "background/watch.js",
      "background/browserEvent.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["src/main.js"]
    }
  ]
}
